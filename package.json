  "scripts": {
    "build:server-win64": "bash build.sh windows amd64",
    "build:server-linux64": "bash build.sh linux amd64",
    "build:server": "npm run build:server-win64 && npm run build:server-linux64",
    "build": "cross-env NODE_ENV=production npm run build:client && npm run build:server",
    "bindata": "go-bindata -debug -o ./server/bindata.go assets/... ./server/templates/...",
    "build:client-dll": "webpack --config=./client/webpack/webpack.dll.js",
    "build:client-app": "webpack --config=./client/webpack/webpack.prod.config.js",
    "build:client": "npm run build:client-dll && npm run build:client-app",
    "dev": "webpack -w --config ./client/webpack/webpack.config.js",
    "start": "node-dev ./client/webpack/webpack.server.js"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^2.2.4",
    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
